<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:id="@id/indeterminate" android:drawable="@drawable/carbon_checkbox_indeterminate" app:carbon_state_indeterminate="true" />
    <item android:state_checked="true" android:id="@id/checked" android:drawable="@drawable/carbon_checkbox_checked" />
    <item android:id="@id/unchecked" android:drawable="@drawable/carbon_checkbox_unchecked" />
    <transition android:drawable="@drawable/carbon_checkbox_unchecked_to_checked" android:toId="@id/checked" android:fromId="@id/unchecked" />
    <transition android:drawable="@drawable/carbon_checkbox_checked_to_unchecked" android:toId="@id/unchecked" android:fromId="@id/checked" />
</animated-selector>